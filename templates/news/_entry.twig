{% extends "_layouts/main" %}

{% block content %}

<section class="news-entry">
  {% if entry.featureImage|length %}
    <img src="{{ entry.featureImage.one().getUrl() }}">
  {% endif %}

  <h1>{{ entry.title }}</h1>

  <div class="news-body-container">
    {{ entry.body }}
  </div>
</section>

{% endblock %}
